{
  "name": "@mikolaj.jeziorny/perfect-autocomplete",
  "version": "1.1.0",
  "description": "A high-quality, accessible autocomplete web component",
  "type": "module",
  "main": "./dist/perfect-autocomplete.umd.js",
  "module": "./dist/perfect-autocomplete.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/perfect-autocomplete.es.js",
      "require": "./dist/perfect-autocomplete.umd.js"
    },
    "./register": {
      "import": "./dist/register.es.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:browser": "vitest --browser",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "typecheck": "vue-tsc --noEmit"
  },
  "keywords": [
    "autocomplete",
    "web-component",
    "custom-element",
    "vue",
    "accessible",
    "a11y"
  ],
  "author": {
    "name": "Mikolaj Jeziorny",
    "url": "https://mikolaj-jeziorny.pl/"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.9",
    "htm": "^3.1.1",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bun": "latest",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/browser": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "ajv": "^8.17.1",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1",
    "semantic-release": "^24.2.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1"
  }
}
